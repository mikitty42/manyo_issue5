<h1>登録済みのユーザー一覧</h1>
<table class='table'>
    <thead>
        <tr>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.created_at %></td>
            </tr>
        <% end %>
    </tbody>
</table>
